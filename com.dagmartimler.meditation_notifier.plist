<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dagmartimler.meditation_notifier</string>
    <key>ProgramArguments</key>
    <array>
        <string><PYTHON_PATH></string>
        <string>/Users/<USERNAME>/Library/Scripts/meditation_notifier.py</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Weekday</key>
        <integer>1</integer>
        <key>Hour</key>
        <integer>9</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>/tmp/meditationnotifier.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/meditationnotifier.error.log</string>
</dict>
</plist>